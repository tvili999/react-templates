<% 
const componentName = args.name.split("/").pop(); 
const contextsDir = (args.contexts || "src/contexts");
-%>
---
to: "<%=contextsDir %>/<%=args.name %>/<%=componentName %>.test.js"
overwrite: true
---
import React from "react";
import tester from "react-utils/tester";
import { <%=componentName %>Provider, <%=componentName %>Consumer } from "./<%= componentName %>.js";

const buildContext = () => tester(Tester => (
    <<%=componentName %>Provider>
        <<%=componentName %>Consumer>
            { value => <Tester value={value}/> }
        </<%=componentName %>Consumer>
    </<%=componentName %>Provider>
));

test('passes', () => {

});
